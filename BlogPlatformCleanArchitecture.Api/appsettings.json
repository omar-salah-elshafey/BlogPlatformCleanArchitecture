{
  "ConnectionStrings": {
    "DefaultConnections": "server=your-server-address; database=your-database-name; integrated security = true; trust server certificate = true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": 1,
  "*",
  "JwtOptions": {
    "Issuer": "your-domain.com",
    "Audience": "your-domain.com",
    "Lifetime": 10,
    "SigningKey": "your-super-secret-key-here"
  },
  "EmailSettings": {
    "SmtpServer": "your-smtp-server",
    "Port": 587,
    "EnableSsl": true,
    "Username": "your-smtp-username",
    "Password": "your-smtp-password",
    "From": "your-email@example.com"
  }
}